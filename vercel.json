{
    "rewrites": [
        {
            "source": "/menu",
            "destination": "menu.html"
        },
        {
            "source": "/contact",
            "destination": "/contact.html"
        }
    ],
    "redirects": [
        {
            "source": "/(.*[^/])",
            "destination": "/$1/",
            "permanent": true
        }
    ]
}